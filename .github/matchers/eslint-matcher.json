{
  "problemMatcher": [
    {
      "owner": "eslint-visualstudio",
      "pattern": {
        "regexp": "^(\\S+)\\((\\d+),(\\d+)\\): (error|warning|info) (\\S+) : (.+)$",
        "file": 1,
        "line": 2,
        "column": 3,
        "severity": 4,
        "message": 6,
        "code": 5
      }
    }
  ]
}